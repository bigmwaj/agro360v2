<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/inventaireCode">
		<property name="editable">
			<list>
				<ref bean="IsBeanCreateOrUpdateOperation"/>
			</list>
		</property>
		<property name="required">
			<list>
				<ref bean="IsTrue" />						
			</list>
		</property>
		<property name="maxLength">
			<list>
				<bean class="com.agro360.operation.metadata.ConstraintConfig">
					<property name="actions">
						<list>
							<bean class="com.agro360.operation.action.common.SetValueAction">
								<constructor-arg value="16"/>											
							</bean>
						</list>
					</property>
				</bean>							
			</list>
		</property>
	</bean>			
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/type">
		<property name="editable">
			<list>
				<ref bean="IsBeanCreateOrUpdateOperation"/>
			</list>
		</property>
	</bean>							
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/name">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />				
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/status">
		<property name="editable">
			<list>
				<ref bean="IsBeanChangeStatusOperation"/>
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/statusDate">
		<property name="editable">
			<list>
				<ref bean="IsBeanChangeStatusOperation"/>
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/firstName">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/lastName">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />					
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/title">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />				
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/phone">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />						
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/email">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />				
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/address">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />						
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/city">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />					
			</list>
		</property>
	</bean>				
		
	<bean class="com.agro360.operation.metadata.FieldMetadataConfig" name="inventaire/country">
		<property name="editable">
			<list>
				<ref bean="IsTrue" />						
			</list>
		</property>
	</bean>		
    
    <bean name="core/inventaire" class="com.agro360.operation.metadata.BeanMetadataConfig">
		<property name="fields">
			<map>
				<entry key="inventaireCode" value-ref="inventaire/inventaireCode"/>
				<entry key="type" value-ref="inventaire/type"/>
				<entry key="name" value-ref="inventaire/name"/>
				<entry key="status" value-ref="inventaire/status"/>
				<entry key="statusDate" value-ref="inventaire/statusDate"/>
				<entry key="firstName" value-ref="inventaire/firstName"/>
				<entry key="lastName" value-ref="inventaire/lastName"/>
				<entry key="title" value-ref="inventaire/title"/>
				<entry key="phone" value-ref="inventaire/phone"/>
				<entry key="email" value-ref="inventaire/email"/>
				<entry key="city" value-ref="inventaire/city"/>
				<entry key="country" value-ref="inventaire/country"/>
			</map>
		</property>
	</bean>
</beans>