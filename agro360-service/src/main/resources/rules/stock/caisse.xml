<?xml version="1.0" encoding="UTF-8"?>
<bean-rules xmlns="http://mouafo.com/xml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://mouafo.com/xml ./../bean-rules.xsd"
	namespace="default" 
	editable="true" 
	visible="true">
	
	<validation>
		<constraint>
			<rule class="com.agro360.service.rule.constraint.common.IsEditableBean"/>
		</constraint>
		<validator class="" lookupClass="com.agro360.service.rule.lookup.common.RootBeanLookup"/>
	</validation>
	
	<validation>
		<constraint>
			<rule class="com.agro360.service.rule.constraint.common.IsEditableBean"/>
		</constraint>
		<validator class="" lookupClass="com.agro360.service.rule.lookup.common.RootBeanLookup"/>
	</validation>
	
	<editableContraints>
		<constraint>
			<rule class="com.agro360.service.rule.constraint.stock.caisse.IsEditableBean"/>
		</constraint>
	</editableContraints>
	
	<bean name="magasin" editable="true" visible="true">
		<field name="magasinCode" required="true" visible="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateAction"/>
				</constraint>
			</editableContraints>
		</field>
		<field name="description" editable="false" visible="true"/>
	</bean>	
	
	<bean name="agent" editable="true" visible="true">
		<field name="tiersCode" required="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateAction"/>
				</constraint>
			</editableContraints>
		</field>
		<field name="tiersName" editable="false" visible="true"/>	
	</bean>
	
	<beanList name="operations" editable="true" visible="true">
			
		<bean name="article" editable="true" visible="true">
			<field name="articleCode" required="true">
				<editableContraints>
					<constraint>
						<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
					</constraint>
				</editableContraints>
			</field>
		</bean>	
		
		<bean name="unite" editable="true" visible="true">
			<field name="uniteCode" required="true">
				<editableContraints>
					<constraint>
						<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
					</constraint>
				</editableContraints>
			</field>	
		</bean>		
		
		<field name="ligneId" required="true" editable="false"/>
	
		<field name="numero" required="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>
		</field>
		
		<field name="typeLigne" required="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>
		</field>
		
		<field name="description">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>
		</field>
		
		<field name="quantite" required="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>
		</field>
		
		<field name="prixUnitaire" required="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>
		</field>
		
		<field name="prixTotal" editable="false"/>
		
		<field name="variantCode">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>
		</field>
		
		<field name="dateOperation" required="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>		
		</field>
		
		<field name="typeOperation" required="true">
			<editableContraints>
				<constraint>
					<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
				</constraint>
			</editableContraints>		
		</field>
	</beanList>
	
	<field name="journee" required="true">
		<editableContraints>
			<constraint>
				<rule class="com.agro360.service.rule.constraint.common.IsCreateAction"/>
			</constraint>
		</editableContraints>
	</field>
	
	<field name="status" required="true">
		<editableContraints>
			<constraint>
				<rule class="com.agro360.service.rule.constraint.common.IsStatusChangeAction"/>
			</constraint>
		</editableContraints>
	</field>
	
	<field name="statusDate" required="true">
		<editableContraints>
			<constraint>
				<rule class="com.agro360.service.rule.constraint.common.IsStatusChangeAction"/>
			</constraint>
		</editableContraints>
	</field>
	
	<field name="note">
		<editableContraints>
			<constraint>
				<rule class="com.agro360.service.rule.constraint.common.IsCreateOrEditAction"/>
			</constraint>
		</editableContraints>
	</field>
	
	<field name="depense" editable="false"/>
	
	<field name="recette" editable="false"/>
	
	<field name="solde" editable="false"/>
	
	<field name="plusVendus"/>
	
	<field name="plusAchetes"/>
</bean-rules>